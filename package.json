{
  "name": "crosscopy",
  "version": "0.0.0",
  "description": "CrossCopy CLI",
  "author": "Huakun Shen @HuakunShen",
  "bin": {
    "xcopy": "./bin/run"
  },
  "homepage": "https://github.com/CrossCopy/crosscopy-client-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "CrossCopy/crosscopy-client-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@crosscopy/core": "^0.0.2-beta2",
    "@crosscopy/graphql-schema": "^0.0.4-beta3",
    "@oclif/core": "^1",
    "@oclif/plugin-autocomplete": "^1.3.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@oclif/plugin-update": "^3.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/uuid": "^8.3.4",
    "clipboardy": "^2.3.0",
    "figlet": "^1.5.2",
    "fs-extra": "^10.1.0",
    "graphql-request": "^4.3.0",
    "graphql-tag": "^2.12.6",
    "inquirer": "^8.2.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-graphql-request": "^4.4.10",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/figlet": "^1.5.4",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.41",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "xcopy",
    "dirname": "xcopy",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-update"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    },
    "update": {
      "s3": {
        "bucket": "crosscopy"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/CrossCopy/crosscopy-client-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
